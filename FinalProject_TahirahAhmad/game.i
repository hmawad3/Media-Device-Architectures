                player.curFrame = 3;
                break;
            }
        }
    }
    for (int j = 0; j < 20; j++) {
        if(enemy3[j].active && !player.hurt) {
            if (collision (enemy3[j].row, enemy3[j].col, enemy3[j].height, enemy3[j].width,
