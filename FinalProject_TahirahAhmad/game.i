                player.hurt = 1;
                updateLives();
                player.curFrame = 3;
                break;
            }
        }
    }
    if (player.hurt == 1) {
        player.hurtTimer++;
        if (player.curFrame < 6 && (player.enemy1Timer % 20 == 0)) {
            player.curFrame++;
        } else if (player.curFrame == 6){
            player.curFrame = 3;
        }
    }
    if (player.hurtTimer >= 100) {
        player.curFrame = 0;
        player.hurt = 0;
        player.hurtTimer = 0;
    }

