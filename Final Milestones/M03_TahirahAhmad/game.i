void initPlayer() {
    player.width = 8;
    player.height = 8;
    player.cdel = 2;
    player.rdel = 2;
    player.row = 125;
    player.col = 118;
    player.hurt = 0;
    player.hurtTimer = 0;
    player.dead = 0;
    player.bulletTimer = 20;
    player.aniCounter = 0;
    player.enemy1Timer = 0;
    player.enemy2Active = 0;
    player.enemy3Active = 0;
    player.curFrame = 0;
    player.numFrames = 1;
    player.shadowOam = 0;
    player.aniState = PLAYERFRONT;
}

void updatePlayer() {
    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5)))
        && player.col >= player.cdel) {

        player.col -= player.cdel;

    } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4)))
        && player.col + player.width < 240 - player.cdel) {

        player.col += player.cdel;

    } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<6)))
        && player.row >= player.rdel && !player.dead) {
        player.row -= player.rdel;

    } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<7)))
        && player.row + player.height - 1 < 160 - 12) {

        player.row += player.rdel;
    }


    if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0)))) && player.bulletTimer >= 25) {
        fireBullet();
        player.bulletTimer = 0;
    }

    player.bulletTimer++;


    if (player.enemy1Timer % 20 == 0) {
        spawnEnemy1();
        if (player.enemy2Active) {
            spawnEnemy2();
        }
        if(player.enemy3Active) {
            spawnEnemy3();
        }
    }
    player.enemy1Timer++;

    for (int i = 0; i < 20; i++) {
        if(enemy1[i].active && !player.hurt) {
            if (collision (enemy1[i].row, enemy1[i].col, enemy1[i].height, enemy1[i].width,
                player.row, player.col, player.height, player.width)) {

                enemy1[i].active = 0;
                shadowOAM[enemy1[i].shadowOam].attr0 |= (2<<8);
                player.hurt = 1;
                updateLives();
                player.curFrame = 1;
                break;
            }
        }
    }
    for (int j = 0; j < 20; j++) {
        if(enemy2[j].active && !player.hurt) {
            if (collision (enemy2[j].row, enemy2[j].col, enemy2[j].height, enemy2[j].width,
                player.row, player.col, player.height, player.width)) {

                enemy2[j].active = 0;
                shadowOAM[enemy2[j].shadowOam].attr0 |= (2<<8);
                player.hurt = 1;
                updateLives();
                player.curFrame = 1;
                break;
            }
        }
    }
    if (player.hurt == 1) {
        player.hurtTimer++;
    }
    if (player.hurtTimer >= 100) {
        player.curFrame = 0;
        player.hurt = 0;
        player.hurtTimer = 0;
    }

}

void initBullet() {

    for (int i = 0; i < 10; i++) {
        bullet[i].row = player.row;
        bullet[i].col = player.col + player.width/2 - bullet[i].width/2;
        bullet[i].rdel = -2;
        bullet[i].height = 8;
        bullet[i].width = 8;
        bullet[i].active = 0;
        bullet[i].aniCounter = 0;
        bullet[i].curFrame = 0;
        bullet[i].numFrames = 1;
        bullet[i].shadowOam = i + 21;
        bullet[i].aniState = BLLTSPRT;

    }
}

void fireBullet() {

    for (int i = 0; i < 10; i++) {
        if (!bullet[i].active) {


            bullet[i].row = player.row;
            bullet[i].col = player.col + player.width/2
                - bullet[i].width/2;


            bullet[i].curFrame = 1;
            bullet[i].active = 1;


            break;
        }
    }
}

void updateBullet(BULLET* b) {


    if (b->active) {
        if (b->row - 5 > 0) {
            b->row += b->rdel;

        } else {
            b->active = 0;
            b->curFrame = 0;
            shadowOAM[b->shadowOam].attr0 |= (2<<8);
        }
    }
}


