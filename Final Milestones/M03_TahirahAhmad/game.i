        }
        if(e->curFrame == 2 )
            e->curFrame = 0;
        if (e->row + e->height >= 160 - 12) {
            e->active = 0;
            shadowOAM[e->shadowOam].attr0 |= (2<<8);
            enemy1onscrn--;
        }
    }
    for (int i = 0; i < 10; i++) {
        if(bullet[i].active) {
            if (collision (bullet[i].row, bullet[i].col, bullet[i].height, bullet[i].width, e->row, e->col, e->height, e->width)) {
                score += 5;
                bullet[i].active = 0;
                e->active = 0;
                shadowOAM[e->shadowOam].attr0 |= (2<<8);
                shadowOAM[bullet[i].shadowOam].attr0 |= (2<<8);
                bullet[i].curFrame = 0;
                enemy1onscrn--;
                e->aniState = ENEMY_1;
            }
        }
    }
